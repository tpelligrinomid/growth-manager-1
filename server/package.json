{
  "name": "growth-manager-server",
  "version": "1.0.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "set PORT=3002 && ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "build:clean": "rm -rf build && mkdir build && npx tsc",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test:connection": "ts-node src/test-connection.ts",
    "postinstall": "npm run build && prisma generate",
    "build:prod": "IF exist build rmdir /s /q build && mkdir build && npx tsc"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "prisma": "^6.2.1",
    "typescript": "^5.7.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.14"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
